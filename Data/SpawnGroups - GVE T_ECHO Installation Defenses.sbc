	<SpawnGroups>
		
		<!-- [NPC Drone] Reaper -->
		
		<SpawnGroup>
			<Id>
				<TypeId>SpawnGroupDefinition</TypeId> <!--Do Not Edit-->
				<SubtypeId>(NPC-GVE)-PlanetDefense-T_ECHO-Reaper</SubtypeId>
			</Id>
			<Description>

			[Modular Encounters SpawnGroup]
			
			[UseKnownPlayerLocations:true]
			[KnownPlayerLocationMustMatchFaction:true]
			[KnownPlayerLocationMinSpawnedEncounters:-1]
			[KnownPlayerLocationMaxSpawnedEncounters:-1]
			
			[AtmosphericCargoShip:true]
			[SkipAirDensityCheck:true]

			[PlanetRequiresAtmo:true]
			[FactionOwner:T_ECHO]
			
			[UseRivalAi:true]
			[RivalAiSpawn:true]
			[RivalAiReplaceRemoteControl:true]
			
			[ReplenishSystems:true]			
			[ReactorsOn:true]			
				
			</Description>
			<IsPirate>true</IsPirate> <!--Do not edit-->
			<Frequency>0.1</Frequency>
			<Prefabs>
				<Prefab SubtypeId="[NPC Drone] Reaper">
					<Position>
						<X>0.0</X>
						<Y>0.0</Y>
						<Z>0.0</Z>
					</Position>
					<Speed>0</Speed>
					<BeaconText>Reaper</BeaconText>					
					<Behaviour>SPRT-FighterBehavior</Behaviour>					
				</Prefab>
			</Prefabs>
		</SpawnGroup>
		
