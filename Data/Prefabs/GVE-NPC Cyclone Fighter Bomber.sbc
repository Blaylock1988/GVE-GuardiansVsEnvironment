public class PDCTurret
{
	public string GroupName;

	public double GridSpeedLimit;

	public IMyMotorStator AzimuthRotor;
	public double LowerLimitX;
	public double UpperLimitX;
	public bool HaveLimitX;

	public ElevationRotorInfo[] ElevationRotors;
	public bool SpareElevationSetup;

	public float RotorVelocityGain;
	public float RotorSpeedLimit;

	public IMyTerminalBlock AimBlock;
	public IMyShipController Controller;

	public List&lt;IMyUserControllableGun&gt; Weapons;
	public WeaponProfile WeaponStats;

	public double FireDotLimit;
	public bool ConstantFireMode;
	public bool ConstantFireLatch;

	public PDCOcclusionChecker GridClearanceCheck;

	public double TargetedCountGainPerHit;

	public PDCTarget TargetInfo;

	public bool IsDamaged;

	public IMyShipConnector ReloadBaseConnector;
	public IMyShipConnector ReloadTurretConnector;

	public int LastReloadCheckClock;
	public int LastReloadOperationCheck;

	public ReloadState CurrentReloadState;
	public double ReloadMaxAngleRadians;
	public double ReloadLockStateAngleRadians;

	public PDCTurret(string groupName, IMyMotorStator azimuthRotor, IMyMotorStator elevationRotor, float rotorVelocityGain, float rotorSpeedLimit, double gridSpeedLimit, IMyTerminalBlock aimBlock, IMyShipController controller, List&lt;IMyUserControllableGun&gt; weapons, WeaponProfile weaponStats, double fireDotLimit, bool constantFireMode, PDCOcclusionChecker gridClearanceCheck = null)
		: this(groupName, azimuthRotor, new List&lt;IMyMotorStator&gt;(new IMyMotorStator[] { elevationRotor }), rotorVelocityGain, rotorSpeedLimit, gridSpeedLimit, aimBlock, controller, weapons, weaponStats, fireDotLimit, constantFireMode, gridClearanceCheck) { }
	public PDCTurret(string groupName, IMyMotorStator azimuthRotor, List&lt;IMyMotorStator&gt; elevationRotors, float rotorVelocityGain, float rotorSpeedLimit, double gridSpeedLimit, IMyTerminalBlock aimBlock, IMyShipController controller, List&lt;IMyUserControllableGun&gt; weapons, WeaponProfile weaponStats, double fireDotLimit, bool constantFireMode, PDCOcclusionChecker gridClearanceCheck = null)
	{
		GroupName = groupName;

		GridSpeedLimit = gridSpeedLimit;
		AimBlock = aimBlock;
		Controller = controller;
		Weapons = weapons;
		WeaponStats = weaponStats;
		FireDotLimit = fireDotLimit;
		ConstantFireMode = constantFireMode;
		RotorVelocityGain = rotorVelocityGain;
		RotorSpeedLimit = rotorSpeedLimit;

		GridClearanceCheck = gridClearanceCheck;

		AzimuthRotor = azimuthRotor;
		GetOrSetRotorLimitConfig(AzimuthRotor, out LowerLimitX, out UpperLimitX);
		HaveLimitX = (LowerLimitX &gt; double.MinValue &amp;&amp; UpperLimitX &lt; double.MaxValue);

		ElevationRotors = new ElevationRotorInfo[elevationRotors.Count];
		for (int i = 0; i &lt; elevationRotors.Count; i++)
		{
			ElevationRotorInfo elevationRotor = new ElevationRotorInfo();
			ElevationRotors[i] = elevationRotor;

			elevationRotor.Rotor = elevationRotors[i];

			Vector3D crossForward = elevationRotors[i].WorldMatrix.Up.Cross(AzimuthRotor.WorldMatrix.Up);

			double forwardAimDot = crossForward.Dot(AimBlock.WorldMatrix.Forward);
			elevationRotor.ReverseY = (forwardAimDot &lt; 0);

			double offsetAngle = Math.Acos(MathHelper.Clamp(forwardAimDot, -1, 1));
			if (AzimuthRotor.WorldMatrix.Up.Dot(AimBlock.WorldMatrix.Forward) &lt; 0)
			{
				offsetAngle = -offsetAngle;
			}
			offsetAngle = elevationRotor.Rotor.Angle - offsetAngle;

			if (elevationRotor.ReverseY) offsetAngle += MathHelperD.Pi;
			MathHelper.LimitRadians2PI(ref offsetAngle);

			elevationRotor.AngleOffsetY = Math.Round(offsetAngle, 2);

			double lowerLimitY, upperLimitY;
			GetOrSetRotorLimitConfig(elevationRotor.Rotor, out lowerLimitY, out upperLimitY);
			elevationRotor.HaveLimitY = (lowerLimitY &gt; double.MinValue &amp;&amp; upperLimitY &lt; double.MaxValue);
			elevationRotor.LowerLimitY = lowerLimitY;
			elevationRotor.UpperLimitY = upperLimitY;
		}
	}

	public void ResetRotors()
	{
		AzimuthRotor.TargetVelocityRad = 0f;
		for (int i = 0; i &lt; ElevationRotors.Length; i++)
		{
			ElevationRotors[i].Rotor.TargetVelocityRad = 0f;
		}
	}

	public bool IsAzimuthRotorIntact()
	{
		return (AzimuthRotor.IsWorking &amp;&amp; AzimuthRotor.IsAttached);
	}

	public bool IsElevationRotorIntact()
	{
		if (ElevationRotors[0].Rotor.IsWorking &amp;&amp; ElevationRotors[0].Rotor.IsAttached)
		{
			return true;
		}
		return false;
	}

	private bool ComputeTargetVectorAndDistance(PDCTarget target, int currentClock, out Vector3D targetVector, out double targetDistance)
	{
		Vector3D shipPosition = AimBlock.WorldMatrix.Translation;

		Vector3D targetPosition = (target.DetectedClock == currentClock ? target.Position : target.Position + ((currentClock - target.DetectedClock) * INV_ONE_TICK * target.Velocity));
		Vector3D targetVelocity = target.Velocity - (Controller == null ? Vector3D.Zero : Controller.GetShipVelocities().LinearVelocity);

		targetPosition = ComputeInterceptPoint(ref targetPosition, ref targetVelocity, ref shipPosition, target, WeaponStats.InitialSpeed, WeaponStats.Acceleration, WeaponStats.MaxSpeed);

		if (double.IsNaN(targetPosition.Sum))
		{
			targetVector = Vector3D.Zero;
			targetDistance = 0;
			return false;
		}
		else
		{
			targetVector = targetPosition - shipPosition;
			targetDistance = targetVector.Length();
			targetVector /= targetDistance;
			return true;
		}
	}

	private bool CheckAndSetAllRotors(double curAzimuth, double curElevation, double aimAzimuth, double aimElevation, bool testOnly = false)
	{
		double yaw;
		if (HaveLimitX)
		{
			if (!GetClampedCorrectionValue(out yaw, aimAzimuth, curAzimuth, LowerLimitX, UpperLimitX))
			{
				return false;
			}
		}
		else
		{
			yaw = aimAzimuth - curAzimuth;

			if (yaw &gt; Math.PI) yaw -= MathHelperD.TwoPi;
			else if (yaw &lt; -Math.PI) yaw += MathHelperD.TwoPi;
		}

		if (!testOnly)
		{
			AzimuthRotor.TargetVelocityRad = Math.Min(RotorSpeedLimit, Math.Max(-RotorSpeedLimit, (float)(yaw * RotorVelocityGain)));
			SetRotorLimits(AzimuthRotor, yaw);
		}

		for (int i = 0; i &lt; ElevationRotors.Length; i++)
		{
			ElevationRotorInfo ElevationRotor = ElevationRotors[i];

			if (ElevationRotor.AngleOffsetY &gt; 0)
			{
				if (ElevationRotor.ReverseY)
				{
					curElevation = ElevationRotor.AngleOffsetY - curElevation;
					if (curElevation &lt; -MathHelperD.TwoPi) curElevation += MathHelperD.TwoPi;

					aimElevation = ElevationRotor.AngleOffsetY - aimElevation;
					if (aimElevation &lt; -MathHelperD.TwoPi) aimElevation += MathHelperD.TwoPi;
				}
				else
				{
					curElevation += ElevationRotor.AngleOffsetY;
					if (curElevation &gt;= MathHelperD.TwoPi) curElevation -= MathHelperD.TwoPi;

					aimElevation += ElevationRotor.AngleOffsetY;
					if (aimElevation &gt;= MathHelperD.TwoPi) aimElevation -= MathHelperD.TwoPi;
				}
			}

			double pitch;
			if (ElevationRotor.HaveLimitY)
			{
				if (!GetClampedCorrectionValue(out pitch, aimElevation, curElevation, ElevationRotor.LowerLimitY, ElevationRotor.UpperLimitY))
				{
					return false;
				}
			}
			else
			{
				pitch = aimElevation - curElevation;

				if (pitch &gt; Math.PI) pitch -= MathHelperD.TwoPi;
				else if (pitch &lt; -Math.PI) pitch += MathHelperD.TwoPi;
			}

			if (!testOnly)
			{
				ElevationRotor.Rotor.TargetVelocityRad = Math.Min(RotorSpeedLimit, Math.Max(-RotorSpeedLimit, (float)(pitch * RotorVelocityGain)));
				SetRotorLimits(ElevationRotor.Rotor, pitch);
			}

			if (!SpareElevationSetup) break;
		}

		return true;
	}

	public bool CheckReloadRequired()
	{
		long startTiming = DateTime.Now.Ticks;

		if (ReloadBaseConnector == null || ReloadTurretConnector == null)
		{
			return false;
		}

		if (ReloadBaseConnector.GetInventory().IsItemAt(0) &amp;&amp; !ReloadTurretConnector.GetInventory().IsItemAt(0))
		{
			return true;
		}

		return false;
	}

	public bool PerformReloadProcedure()
	{
		long startTiming = DateTime.Now.Ticks;

		if (ReloadBaseConnector == null || ReloadTurretConnector == null)
		{
			return true;
		}

		if (IsDamaged)
		{
			CurrentReloadState = ReloadState.NONE;
			return true;
		}
		else if (!IsAzimuthRotorIntact() || !IsElevationRotorIntact())
		{
			CurrentReloadState = ReloadState.NONE;
			IsDamaged = true;
			return true;
		}

		if (CurrentReloadState == ReloadState.NONE)
		{
			ReleaseWeapons(true);
			CurrentReloadState = ReloadState.MAX_ANGLE;
		}

		double curAzimuth, curElevation, aimElevation;

		switch (CurrentReloadState)
		{
			case ReloadState.MAX_ANGLE:
				GetAzimuthElevation(AimBlock.WorldMatrix.Forward, AzimuthRotor, out curAzimuth, out curElevation);
				aimElevation = ReloadMaxAngleRadians;

				if (Math.Abs(aimElevation - curElevation) &gt; 0.0018)
				{
					CheckAndSetAllRotors(curAzimuth, curElevation, curAzimuth, aimElevation, false);
				}
				else
				{
					CurrentReloadState = ReloadState.LOCK_STATE_ANGLE;
				}

				break;
			case ReloadState.LOCK_STATE_ANGLE:
				GetAzimuthElevation(AimBlock.WorldMatrix.Forward, AzimuthRotor, out curAzimuth, out curElevation);
				aimElevation = ReloadLockStateAngleRadians;

				if (Math.Abs(aimElevation - curElevation) &gt; 0.0018)
				{
					CheckAndSetAllRotors(curAzimuth, curElevation, curAzimuth, aimElevation, false);
				}
				else
				{
					CurrentReloadState = ReloadState.LOCK_CONNECTORS;
				}

				break;
			case ReloadState.LOCK_CONNECTORS:
				if (ReloadTurretConnector.Status == MyShipConnectorStatus.Connected)
				{
					CurrentReloadState = ReloadState.TRANSFER_ITEMS;
				}
				else if (ReloadTurretConnector.Status == MyShipConnectorStatus.Connectable)
				{
					ReloadTurretConnector.Connect();
				}
				break;
			case ReloadState.TRANSFER_ITEMS:
				if (ReloadBaseConnector.GetInventory().IsItemAt(0))
				{
					ReloadTurretConnector.GetInventory().TransferItemFrom(ReloadBaseConnector.GetInventory(), 0, 0, true);
				}
				ReloadTurretConnector.Disconnect();
				ReloadBaseConnector.Disconnect();
				CurrentReloadState = ReloadState.NONE;
				return true;
		}

		return false;
	}

	public bool IsTargetable(PDCTarget target, int currentClock)
	{
		long startTiming = DateTime.Now.Ticks;

		if (IsDamaged)
		{
			return false;
		}
		else if (!IsAzimuthRotorIntact() || !IsElevationRotorIntact())
		{
			IsDamaged = true;
			return false;
		}

		if (CurrentReloadState != ReloadState.NONE)
		{
			return false;
		}

		Vector3D targetVector;
		double targetDistance;
		bool interceptable = ComputeTargetVectorAndDistance(target, currentClock, out targetVector, out targetDistance);

		if (interceptable)
		{
			if (GridClearanceCheck != null &amp;&amp; GridClearanceCheck.TestWorldRayHit(AimBlock.WorldMatrix.Translation, targetVector))
			{
				return false;
			}

			if (HaveLimitX || ElevationRotors[0].HaveLimitY)
			{
				double curAzimuth, curElevation;
				double aimAzimuth, aimElevation;
				GetAzimuthElevation(AimBlock.WorldMatrix.Forward, AzimuthRotor, out curAzimuth, out curElevation);
				GetAzimuthElevation(targetVector, AzimuthRotor, out aimAzimuth, out aimElevation);

				if (!CheckAndSetAllRotors(curAzimuth, curElevation, aimAzimuth, aimElevation, true))
				{
					return false;
				}
			}

			if (targetDistance &lt;= WeaponStats.MaxRange)
			{
				return true;
			}
		}

		return false;
	}

	public bool AimAndFire(PDCTarget target, int currentClock)
	{
		long startTiming = DateTime.Now.Ticks;

		if (IsDamaged)
		{
			ReleaseWeapons(true);
			ResetRotors();
			return false;
		}
		else if (!IsAzimuthRotorIntact() || !IsElevationRotorIntact())
		{
			ReleaseWeapons(true);
			ResetRotors();
			IsDamaged = true;
			return false;
		}

		if (CurrentReloadState != ReloadState.NONE)
		{
			return false;
		}

		Vector3D targetVector;
		double targetDistance;
		bool interceptable = ComputeTargetVectorAndDistance(target, currentClock, out targetVector, out targetDistance);

		if (interceptable)
		{
			if (GridClearanceCheck != null &amp;&amp; GridClearanceCheck.TestWorldRayHit(AimBlock.WorldMatrix.Translation, targetVector))
			{
				ReleaseWeapons();
				ResetRotors();
				return false;
			}

			double curAzimuth, curElevation;
			double aimAzimuth, aimElevation;
			GetAzimuthElevation(AimBlock.WorldMatrix.Forward, AzimuthRotor, out curAzimuth, out curElevation);
			GetAzimuthElevation(targetVector, AzimuthRotor, out aimAzimuth, out aimElevation);

			bool setRotorSuccess = CheckAndSetAllRotors(curAzimuth, curElevation, aimAzimuth, aimElevation, false);

			if (setRotorSuccess)
			{
				if (targetDistance &lt;= WeaponStats.MaxRange)
				{
					if (AimBlock.WorldMatrix.Forward.Dot(targetVector) &gt;= FireDotLimit)
					{
						FireWeapons();

						target.PDCTargetedCount += TargetedCountGainPerHit;
					}
					return true;
				}
			}
		}

		ReleaseWeapons();
		ResetRotors();
		return false;
	}

	public Vector3D ComputeInterceptPoint(ref Vector3D targetLocation, ref Vector3D targetVelocity, ref Vector3D projectileLocation, PDCTarget target, double projectileInitialSpeed, double projectileAcceleration, double projectileMaxSpeed)
	{
		//---------- Calculate Impact Point ----------

		//targetDirection Must Be Normalized
		Vector3D z = targetLocation - projectileLocation;
		double k = (projectileAcceleration == 0 ? 0 : (projectileMaxSpeed - projectileInitialSpeed) / projectileAcceleration);
		double p = (0.5 * projectileAcceleration * k * k) + (projectileInitialSpeed * k) - (projectileMaxSpeed * k);

		double a = (projectileMaxSpeed * projectileMaxSpeed) - targetVelocity.LengthSquared();
		double b = 2 * ((p * projectileMaxSpeed) - targetVelocity.Dot(z));
		double c = (p * p) - z.LengthSquared();

		double t = SolveQuadratic(a, b, c);

		if (Double.IsNaN(t) || t &lt; 0)
		{
			return new Vector3D(Double.NaN, Double.NaN, Double.NaN);
		}
		else
		{
			if (target.Acceleration.LengthSquared() &gt; 0.1)
			{
				return targetLocation + (targetVelocity * t) + (0.5 * target.Acceleration * t * t);
			}
			else
			{
				return targetLocation + (targetVelocity * t);
			}
		}
	}

	public double SolveQuadratic(double a, double b, double c)
	{
		double u = (b * b) - (4 * a * c);
		if (u &lt; 0)
		{
			return Double.NaN;
		}
		u = Math.Sqrt(u);

		double t1 = (-b + u) / (2 * a);
		double t2 = (-b - u) / (2 * a);
		return (t1 &gt; 0 ? (t2 &gt; 0 ? (t1 &lt; t2 ? t1 : t2) : t1) : t2);
	}

	public void GetAzimuthElevation(Vector3D aimVector, IMyMotorStator azRotor, out double azimuth, out double elevation)
	{
		double dot = aimVector.Dot(azRotor.WorldMatrix.Up);
		elevation = MathHelperD.PiOver2 - Math.Acos(MathHelper.Clamp(dot, -1, 1));

		Vector3D azForward = (aimVector - (azRotor.WorldMatrix.Up * dot)) / Math.Cos(elevation);
		azimuth = Math.Acos(MathHelper.Clamp(azRotor.WorldMatrix.Backward.Dot(azForward), -1, 1));
		if (azRotor.WorldMatrix.Right.Dot(azForward) &gt; 0) azimuth = MathHelperD.TwoPi - azimuth;
	}

	public bool GetClampedCorrectionValue(out double correction, double desiredAngle, double currentAngle, double lowerLimit, double upperLimit)
	{
		upperLimit -= lowerLimit;
		desiredAngle -= lowerLimit;
		currentAngle -= lowerLimit;

		Limit2PILite(ref upperLimit);
		Limit2PILite(ref desiredAngle);
		Limit2PILite(ref currentAngle);

		if (desiredAngle &gt;= 0 &amp;&amp; desiredAngle &lt;= upperLimit)
		{
			if (currentAngle &gt; upperLimit)
			{
				currentAngle = (currentAngle - upperLimit &lt;= MathHelperD.TwoPi - currentAngle ? upperLimit : 0);
			}

			correction = desiredAngle - currentAngle;
			return true;
		}
		else
		{
			correction = 0;
			return false;
		}
	}

	public void SetRotorLimits(IMyMotorStator rotor, double diffAngle)
	{
		float desiredAngle = rotor.Angle + (float)diffAngle;

		if (desiredAngle &lt; -MathHelper.TwoPi || desiredAngle &gt; MathHelper.TwoPi)
		{
			rotor.SetValueFloat("LowerLimit", float.MinValue);
			rotor.SetValueFloat("UpperLimit", float.MaxValue);
		}
		else if (diffAngle &lt; 0)
		{
			rotor.UpperLimitRad = rotor.Angle;
			rotor.LowerLimitRad = desiredAngle;
		}
		else if (diffAngle &gt; 0)
		{
			rotor.LowerLimitRad = rotor.Angle;
			rotor.UpperLimitRad = desiredAngle;
		}
	}

	public void GetOrSetRotorLimitConfig(IMyMotorStator rotor, out double lowerLimit, out double upperLimit)
	{
		double lowerLimitDeg = double.MinValue;
		double upperLimitDeg = double.MaxValue;

		CustomConfiguration cfg = new CustomConfiguration(rotor);
		cfg.Load();

		cfg.Get("LowerLimit", ref lowerLimitDeg);
		cfg.Get("UpperLimit", ref upperLimitDeg);

		if (lowerLimitDeg == double.MinValue || upperLimitDeg == double.MaxValue)
		{
			cfg.Set("LowerLimit", MathHelper.Clamp(Math.Round(rotor.LowerLimitDeg, 3), -361, 361).ToString());
			cfg.Set("UpperLimit", MathHelper.Clamp(Math.Round(rotor.UpperLimitDeg, 3), -361, 361).ToString());
			cfg.Save();
		}

		if (lowerLimitDeg &lt; -360) lowerLimitDeg = double.MinValue;
		if (upperLimitDeg &gt; 360) upperLimitDeg = double.MaxValue;

		if (upperLimitDeg &lt; lowerLimitDeg || upperLimitDeg - lowerLimitDeg &gt;= 360)
		{
			lowerLimitDeg = double.MinValue;
			upperLimitDeg = double.MaxValue;
		}

		if (lowerLimitDeg &gt; double.MinValue &amp;&amp; upperLimitDeg &lt; double.MaxValue)
		{
			lowerLimit = MathHelperD.ToRadians(lowerLimitDeg);
			upperLimit = MathHelperD.ToRadians(upperLimitDeg);
		}
		else
		{
			lowerLimit = double.MinValue;
			upperLimit = double.MaxValue;
		}
	}

	public void Limit2PILite(ref double value)
	{
		if (value &lt; 0)
		{
			if (value &lt; -MathHelperD.TwoPi) value += MathHelperD.FourPi;
			else value += MathHelperD.TwoPi;
		}
		else if (value &gt; MathHelperD.TwoPi)
		{
			if (value &gt; MathHelperD.FourPi) value -= MathHelperD.FourPi;
			else value -= MathHelperD.TwoPi;
		}
	}

	public void FireWeapons()
	{
		if (ConstantFireMode)
		{
			if (!ConstantFireLatch)
			{
				foreach (IMyUserControllableGun weapon in Weapons)
				{
					weapon.SetValueBool("Shoot", true);
				}
				ConstantFireLatch = true;
			}
		}
		else
		{
			foreach (IMyUserControllableGun weapon in Weapons)
			{
				weapon.ApplyAction("ShootOnce");
			}
		}
	}

	public void ReleaseWeapons(bool force = false)
	{
		if (ConstantFireLatch || force)
		{
			foreach (IMyUserControllableGun weapon in Weapons)
			{
				weapon.SetValueBool("Shoot", false);
			}
			ConstantFireLatch = false;
		}
	}

