Program()
{
    Runtime.UpdateFrequency = UpdateFrequency.Update1;

    _runtimeTracker = new RuntimeTracker(this);

    _artificialHorizon = new ArtificialHorizon(
            _sunRotationAxis,
            _skyColor,
            _groundColor,
