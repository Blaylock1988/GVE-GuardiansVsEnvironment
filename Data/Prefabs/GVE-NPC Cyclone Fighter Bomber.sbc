void RedrawStructure()
{
if (mode == 3 || mode == 4)
{
for (int i = 0; i &lt; lyts.Count; i++)
{
lyts[i].HideLargeNumber();
}
}

for (int i = pnStart; i &lt;= pnEnd; i++)
{
