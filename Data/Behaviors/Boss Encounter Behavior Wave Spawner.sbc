      [Color:White]
      
      [ChatMessages:{GPS}]
      [ChatAudio:ArcHudGPSNotification1]
      [BroadcastChatType:Chat]
	  [SendToAllOnlinePlayers:true]
	  [GPSLabel:Pirate Hoard Battlefield]

      </Description>      
    </EntityComponent>
	
	<!--Trigger sequence to enable triggers and begin counters and waves-->
