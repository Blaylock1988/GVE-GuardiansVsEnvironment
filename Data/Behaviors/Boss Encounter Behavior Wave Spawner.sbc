<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <EntityComponents>
	
	<!--=== Pre-wave setup ===-->
	
	<!--This wave spanwer is station with reduced damage that the player must defend from
	10 waves of pirates of increasing difficulty. If the player completes all waves, then 
	a loot drop reward will	spawn. Balance is required to make it challenging yet also 
	avoid NPC spam swarm. You should use incredibly aggressive MES Cleanup to help minimize
	impact on the server. In my mod a strong fleet of allies is required to make it to the
	very end. Players will need to coordinate their movement as well resupply and repair 
	runs to be successful. If the player attacks the station or fails to defend it then
	they will lose reputation when the station is destroyed.-->
	
	<!--Set up the behavior for the station that will act as the target and pirate spawner-->
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Behavior-Start</SubtypeId>
      </Id>
      <Description>

      [RivalAI Behavior]
	  
      [BehaviorName:Passive]
	  [UsePlayerDistanceTimer:true]
	  [PlayerDistanceTimerTrigger:5]
	  [PlayerDistanceTrigger:50000]
	  [UseNoTargetTimer:false]
	  [NoTargetTimerTrigger:600]
	  [UseRetreatTimer:false]
	  
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-PlayerDistant]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-StartWaves]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-NoPiratesRemaining]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-NextWaveCountdown]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-TimeWarning20]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-TimeWarning15]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-TimeWarning10]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-TimeWarning05]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-TimeWarning01] 
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-IncreaseWaveNumber]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave01]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave02]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave03]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave04]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave05]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave06]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave07]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave08]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave09]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave10]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave01Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave02Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave03Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave04Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave05Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave06Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave07Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave08Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave09Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave10Timer]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Reward]
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-FighterDefenses]	 
	  [Triggers:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Defeated]
	  [Triggers:GVE-Generic-Trigger-GVE+TakeDamage]

	  [TargetData:GVE-RE-Boss-WaveSpawnerDefend-TargetProfile-PirateDetection]
	  
      </Description>
    </EntityComponent>
	
	<!--Target profile for detecting hostile NPC RAI RCs spawned by waves-->
    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-TargetProfile-PirateDetection</SubtypeId>
      </Id>
      <Description>

      [RivalAI Target]
      
      [UseCustomTargeting:true]
      [Target:Block]
	  [BlockTargets:RivalAi]
	  [UseTargetRefresh:false]
	  [TimeUntilNextRefresh:30]
	  [TimeUntilNextEvaluation:0]
      [MaxDistance:10000]
	  
      [MatchAllFilters:Faction]
      [FactionTargets:SPRT]
	  [GetTargetBy:ClosestDistance]
	  
      </Description>
    </EntityComponent>
	
	<!--Trigger sequence to message players and provide GPS coord upon station spawn-->
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Trigger-PlayerDistant</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  
      [Type:PlayerNear]
	  [UseTrigger:true]
	  [TargetDistance:50000]
	  [MaxActions:1]
	  
	  [Actions:GVE-RE-Boss-WaveSpawnerDefend-Action-PlayerDistant]
	  
      </Description>      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Action-PlayerDistant</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
	  
	  [ChangeAntennaRanges:true]
	  [AntennaNamesForRangeChange:NPC Antenna]
	  [AntennaRangeChangeType:Set]
	  [AntennaRangeChangeAmount:50000]
	  
      [UseChatBroadcast:true]
      [ChatData:GVE-RE-Boss-WaveSpawnerDefend-Chat-PlayerDistant]
	  [ChatData:GVE-RE-Boss-WaveSpawnerDefend-Chat-PlayerDistantGPS]

      </Description>
    </EntityComponent>
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Chat-PlayerDistant</SubtypeId>
      </Id>
      <Description>

      [RivalAI Chat]
      
      [UseChat:true]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:1]
      [BroadcastRandomly:false]
      [Author:{GridName}]
      [Color:White]
      
      [ChatMessages:Please help! Pirates are about to attack the station!]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Both]
	  [SendToAllOnlinePlayers:true]

      </Description>      
    </EntityComponent>

	<!--GPS only via chat instead of notify-->
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Chat-PlayerDistantGPS</SubtypeId>
      </Id>
      <Description>

