    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawner-Action-StartWaves</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
	  	  
	  [ChangeTargetProfile:true]
	  [RefreshTarget:true]
	  [NewTargetProfileId:GVE-RE-Boss-WaveSpawner-TargetProfile-PirateDetection]
	  
	  [SetBooleansFalse:NoPiratesRemaining]
	  
	  [EnableTriggers:true]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-NoPiratesRemainingTrue]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-NoPiratesRemainingFalse]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave1A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave2A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave3A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave4A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave4B]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave5A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave5B]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave6A]	  
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave6B]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave7A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave7B]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave8A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave8B]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave9A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave9B]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave10A]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Wave10B]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawner-Trigger-Reward]
		  
      [UseChatBroadcast:true]
      [ChatData:GVE-RE-Boss-WaveSpawner-Chat-StartWaves]

      </Description>
    </EntityComponent>
	
