          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Action-NextWaveCountdown</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
	  
	  [DecreaseCounters:TimeCounter]
	  	  
      </Description>
    </EntityComponent>
		
