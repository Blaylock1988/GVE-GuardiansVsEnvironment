      [ChatData:GVE-RE-Boss-WaveSpawnerDefend-Chat-StartWaves]

      </Description>
    </EntityComponent>
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Chat-StartWaves</SubtypeId>
      </Id>
      <Description>

