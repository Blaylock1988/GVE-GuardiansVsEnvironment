      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Trigger-NextWaveCountdown</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  
      [Type:Timer]
	  [UseTrigger:false]
	  [StartsReady:true]
	  [MinCooldownMs:60000]
	  [MaxCooldownMs:60000]
	  	 	  
	  [Actions:GVE-RE-Boss-WaveSpawnerDefend-Action-NextWaveCountdown]
	  
      </Description>      
    </EntityComponent>
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
