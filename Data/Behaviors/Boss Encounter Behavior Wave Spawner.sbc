      [UseChat:true]
      [StartsReady:true]
      [Chance:100]
      [MaxChats:1]
      [BroadcastRandomly:false]
      [Author:{GridName}]
      [Color:White]
      
      [ChatMessages:Pirates indbound. Event will end if no players remain within 50km of the battlefield.]
      [ChatAudio:ArcHudGPSNotification2]
      [BroadcastChatType:Both]
	  	  
      </Description>      
    </EntityComponent>
	
	<!--When last enemy is dead, trigger the next wave early with 1 minute delay-->
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Trigger-NoPiratesRemaining</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  
      [Type:NoTarget]
	  [UseTrigger:false]
	  [StartsReady:false]
	  [MinCooldownMs:30000]
	  [MaxCooldownMs:30000]
	  
	  [Conditions:GVE-RE-Boss-WaveSpawnerDefend-Condition-NoPiratesRemaining]
	  [ConditionCheckResetsTimer:true]
	  
	  [Actions:GVE-RE-Boss-WaveSpawnerDefend-Action-NoPiratesRemaining]
	  
      </Description>      
    </EntityComponent>
	
	<!--condition prevents the waves from going past the reward at the end-->
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Condition-NoPiratesRemaining</SubtypeId>
      </Id>
      <Description>

      [RivalAI Condition]
      
      [UseConditions:true]
      [MatchAnyCondition:false]
	  [CheckCustomCounters:true]
	  [CustomCounters:WaveCounter]
	  [CounterCompareTypes:Less]
	  [CustomCountersTargets:11]
	        
      </Description>
    </EntityComponent>
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Action-NoPiratesRemaining</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
	  	  	  
	  [SetCounters:TimeCounter]
	  [SetCountersValues:1]
	  
      </Description>
    </EntityComponent>
						
	<!--Countdown for time warnings and wave increment, triggers every 60 seconds-->
