	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawnerDefend-Trigger-TimeWarning05]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawnerDefend-Trigger-TimeWarning01]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawnerDefend-Trigger-IncreaseWaveNumber]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave01]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawnerDefend-Trigger-Wave02]
