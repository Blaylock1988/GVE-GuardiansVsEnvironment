      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Trigger-StartWaves</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  
      [Type:PlayerNear]
	  [UseTrigger:true]
	  [TargetDistance:2000]
	  [MaxActions:1]
	  
	  [Actions:GVE-RE-Boss-WaveSpawnerDefend-Action-StartWaves]
	  
      </Description>      
    </EntityComponent>

	<!-- enable the rest of the wave trigger; this is required so that the NoTarget/Timer triggers below do not trigger upon spawn-->
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-WaveSpawnerDefend-Action-StartWaves</SubtypeId>
      </Id>
      <Description>

      [RivalAI Action]
	  
	  [SetCounters:TimeCounter]
	  [SetCountersValues:0]
	  	  
	  [EnableTriggers:true]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawnerDefend-Trigger-NoPiratesRemaining]
	  [EnableTriggerNames:GVE-RE-Boss-WaveSpawnerDefend-Trigger-NextWaveCountdown]
