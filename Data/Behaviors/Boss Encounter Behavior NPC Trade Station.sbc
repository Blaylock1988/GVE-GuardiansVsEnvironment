    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-NPCTradeStation-Spawner-Defenses</SubtypeId>
      </Id>
      <Description>

      [RivalAI Spawn]
      
      [UseSpawn:true]
      
	  [SpawningType:RandomEncounter]
      [StartsReady:true]
      [SpawnMinCooldown:60]
      [SpawnMinCooldown:120]
      [MaxSpawns:8]
            
      [MinDistance:2000]
      [MaxDistance:4000]
      [IgnoreSafetyChecks:false]
      
      </Description>
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-Boss-NPCTradeStation-Trigger-EnemyNear</SubtypeId>
      </Id>
      <Description>

      [RivalAI Trigger]
	  
      [Type:PlayerNear]
	  [UseTrigger:true]
	  [TargetDistance:5000]
	  [MinCooldownMs:15000]
	  [MaxCooldownMs:30000]
	  [StartsReady:false]
	  [MaxActions:1]
	  
	  [MinPlayerReputation:-1501]
	  [MaxPlayerReputation:-500]
	  [CustomReputationRangeCheck:5000]
	  
	  [Actions:GVE-RE-Boss-NPCTradeStation-Action-EnemyNear]
	  
      </Description>      
    </EntityComponent>

	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
