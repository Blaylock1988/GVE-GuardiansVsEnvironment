  <EntityComponents>

	<!-- MONK Large Ship Behaviors -->
	
	<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
          <TypeId>Inventory</TypeId>
          <SubtypeId>GVE-RE-MONK-Proteus-Behavior-Start</SubtypeId>
      </Id>
      <Description>

      [RivalAI Behavior]
	  
      [BehaviorName:Horsefly]

	  [HorseflyWaypointWaitTimeTrigger:0]

	  [UseVelocityCollisionEvasion:true]
	  [CollisionEvasionWaypointDistance:200]
	  [CollisionEvasionResumeDistance:50]
	  [CollisionEvasionResumeTime:10]
	  [WaypointTolerance:100]
	  [UseProjectileLeadPrediction:false]
	  [OffsetSpaceMinDistFromTarget:400]
	  [OffsetSpaceMaxDistFromTarget:1000]
	  [RotationMultiplier:1.5]	  	
	  [IdealMaxSpeed:70]
	  [IdealMinSpeed:40]
	  [MaxSpeedTolerance:10]
	  [SlowDownOnWaypointApproach:true]
	  [ExtraSlowDownDistance:0]
	  [AllowStrafing:false]
	  [AngleAllowedForForwardThrust:50]
	  
	  [UseStaticGuns:false]
	  [UseTurrets:true]
	  [UseAmmoReplenish:true]
	  [AmmoReplenishClipAmount:100]
	  
	  [UsePlayerDistanceTimer:true]
	  [PlayerDistanceTimerTrigger:5]
	  [PlayerDistanceTrigger:50000]
	  [UseNoTargetTimer:true]
	  [NoTargetTimerTrigger:600]
	  [UseRetreatTimer:false]
	  
	  [TargetData:GVE-Generic-TargetProfile-EnemyPlayers]
		  
	  [Triggers:GVE-Generic-Trigger-MONKTakeDamage]
	  [Triggers:GVE-Generic-Trigger-MONKCallDDSFighters]
	  [Triggers:GVE-Generic-Trigger-SPRTDefeated]
	  [Triggers:GVE-Generic-Trigger-SPRTNoWeapon]
	  
      </Description>
    </EntityComponent>
			
